    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "pnpm": "^8.6.12",
    "prettier": "^3.0.3",
    "rollup-plugin-visualizer": "^5.9.2",
    "taze": "^0.11.2",
    "terser": "^5.19.2",
